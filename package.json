{
  "name": "code-sharing-spike",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "pnpm nx start bookmarks",
    "build": "pnpm nx build bookmarks",
    "format": "nx run-many --target=format",
    "format-check": "nx run-many --target=format-check",
    "check-types": "tsc -b",
    "lint": "nx run-many --target=lint",
    "test": "nx run-many --target=test",
    "storybook": "storybook dev -p 6100",
    "build-storybook": "storybook build",
    "cypress:open": "pnpm cypress open -P ./apps/bookmarks-e2e",
    "cypress:run": "pnpm cypress run -P ./apps/bookmarks-e2e",
    "clean": "find . -type d -name 'dist' -not -path '*/node_modules/*' -exec rm -r {} + && rm -rf node_modules/.cache"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
